{
  "name": "julinemart-logistics-orchestrator",
  "version": "1.0.0",
  "type": "module",
  "description": "Multi-hub logistics management system for JulineMart marketplace",
  "main": "dist/api/index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "server": "node server.js",
    "test": "vitest",
    "lint": "eslint src --ext ts,tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:db:reset": "supabase db reset",
    "supabase:gen-types": "supabase gen types typescript --local > src/types/supabase.ts",
    "netlify:dev": "netlify dev",
    "api:dev": "tsx watch src/api/index.ts",
    "api:start": "node dist/api/index.js",
    "portal:dev": "vite --config vite.config.customer.ts",
    "portal:build": "tsc && vite build --config vite.config.customer.ts",
    "portal:preview": "vite preview --config vite.config.customer.ts",
    "dev:all": "powershell -NoProfile -Command \"Start-Process npm -ArgumentList 'run','api:dev'; Start-Process npm -ArgumentList 'run','dev'; Start-Process npm -ArgumentList 'run','portal:dev'\""
  },
  "keywords": [
    "logistics",
    "woocommerce",
    "supabase",
    "multi-vendor"
  ],
  "author": "JulineMart",
  "license": "MIT",
  "dependencies": {
    "2": "^3.0.0",
    "@supabase/supabase-js": "^2.86.0",
    "@tanstack/react-query": "^5.90.7",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "lucide-react": "^0.553.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.10",
    "pdfkit": "^0.17.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.6.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.30.1",
    "recharts": "^3.3.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/express": "^4.17.25",
    "@types/node": "^20.19.24",
    "@types/nodemailer": "^7.0.3",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "supabase": "^1.226.4",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vitest": "^1.6.1"
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  }
}
